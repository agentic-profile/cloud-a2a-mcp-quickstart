{
  "name": "agentic-profile-mcp",
  "version": "1.0.0",
  "description": "Agentic Profile MCP Lambda Function",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "jest",
    "test:redis": "node scripts/test-redis-connection.js",
    "test:local-redis": "npm run build && node scripts/test-local-redis.js",
    "test:local-dynamodb": "npm run build && node scripts/test-local-dynamodb.js",
    "dev": "tsx --inspect src/index.local.ts",
    "setup-well-known-agent": "tsx scripts/setup-well-known-agent.js"
  },
  "keywords": [
    "lambda",
    "typescript",
    "mcp"
  ],
  "author": "Mike Prince",
  "license": "Apache 2.0",
  "dependencies": {
    "@a2a-js/sdk": "^0.3.2",
    "@agentic-profile/auth": "^0.6.2",
    "@agentic-profile/common": "^0.6.0",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-cloudformation": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-secrets-manager": "^3.859.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "@codegenie/serverless-express": "^4.17.0",
    "@digitalbazaar/ed25519-signature-2020": "^5.4.0",
    "@digitalbazaar/ed25519-verification-key-2020": "^4.2.0",
    "@digitalbazaar/vc": "^7.2.0",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "loglevel": "^1.9.2",
    "mathjs": "^14.8.2",
    "redis": "^5.7.0"
  },
  "devDependencies": {
    "@agentic-profile/express-common": "^0.6.1",
    "@types/aws-lambda": "^8.10.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "esbuild": "^0.25.9",
    "jest": "^29.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
